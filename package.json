{
  "name": "cryptic-website",
  "version": "1.0.0",
  "description": "The website of the Cryptic game",
  "author": "The Cryptic Team",
  "license": "GPL-3",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch server/dist server/dist/main.js",
    "watch": "babel server/src --out-dir server/dist --copy-files --watch",
    "build": "yarn build:server && yarn build:nuxt",
    "build:server": "babel server/src --out-dir server/dist --copy-files",
    "build:nuxt": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/dist/main.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.9.5",
    "@nuxtjs/pwa": "^3.0.0-0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "kiste": "^1.2.7",
    "lodash.omit": "^4.5.0",
    "mariadb": "^2.1.5",
    "nuxt": "^2.11.0",
    "sequelize": "^5.21.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@moritzruth/eslint-config": "^1.0.8",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.1.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-vue": "^6.0.1",
    "fibers": "^4.0.2",
    "nodemon": "^2.0.2",
    "sass": "^1.23.3",
    "sass-loader": "^8.0.0",
    "vue-svg-loader": "^0.14.0"
  }
}
